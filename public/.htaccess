<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  # Skip the rewrite if the file exists (like google9ecb084a3d7bb9be.html)
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  # Exclude Google's verification file explicitly
  RewriteCond %{REQUEST_URI} !^/google9ecb084a3d7bb9be\.html$
  RewriteRule ^ index.html [L]
</IfModule>
